# Example terraform.tfvars file for Staging Environment
# Copy this file to terraform.tfvars and customize the values as needed

# AWS Configuration
aws_region  = "us-east-2"
aws_profile = "robotics-protocol-library-prod-root"

# Environment Configuration
environment = "staging"
domain_name = "staging.docs.opentrons.com"
mkdocs_bucket_name = "opentrons.staging.docs"
labware_library_bucket_name = "opentrons.staging.labware.library"

# S3 Configuration
enable_versioning                    = true
enable_lifecycle_rules               = true
noncurrent_version_expiration_days   = 14

# CloudFront Configuration
cloudfront_function_arn = "arn:aws:cloudfront::043748923082:function/indexRedirect"

# SSL/TLS Configuration
acm_certificate_arn = "arn:aws:acm:us-east-1:043748923082:certificate/5edbb417-cdbd-4fbc-8f19-18fbc169c531"

# WAF Configuration (not configured for staging)
web_acl_id = null
