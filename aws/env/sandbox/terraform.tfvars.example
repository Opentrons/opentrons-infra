# Example terraform.tfvars file for Sandbox Environment
# Copy this file to terraform.tfvars and customize the values as needed

# AWS Configuration
aws_region  = "us-east-2"
aws_profile = "robotics-protocol-library-prod-root"

# Environment Configuration
environment = "sandbox"
domain_name = "sandbox.docs.opentrons.com"
labware_library_domain_name = "sandbox.labware.opentrons.com"
mkdocs_bucket_name = "sandbox.docs"
labware_library_bucket_name = "sandbox.labware"

# S3 Configuration
enable_versioning                    = true
enable_lifecycle_rules               = true
noncurrent_version_expiration_days   = 7

# CloudFront Configuration
cloudfront_function_arn = "arn:aws:cloudfront::043748923082:function/indexRedirect"

# SSL/TLS Configuration
acm_certificate_arn = "arn:aws:acm:us-east-1:043748923082:certificate/fefdb546-2e50-46fc-8781-efd96521e779"

# WAF Configuration (not configured for sandbox)
web_acl_id = null
