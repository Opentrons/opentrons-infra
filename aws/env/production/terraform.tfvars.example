# Example terraform.tfvars file for Production Environment
# Copy this file to terraform.tfvars and customize the values as needed

# AWS Configuration
aws_region  = "us-east-2"
aws_profile = "robotics-protocol-library-prod-root"

# Environment Configuration
environment = "production"
domain_name = "docs.opentrons.com"
bucket_name = "opentrons.production.docs"

# S3 Configuration
enable_versioning                    = true
enable_lifecycle_rules               = true
noncurrent_version_expiration_days   = 30

# CloudFront Configuration
cloudfront_function_arn = "arn:aws:cloudfront::043748923082:function/indexRedirect"

# SSL/TLS Configuration
acm_certificate_arn = "arn:aws:acm:us-east-1:043748923082:certificate/ddb7db56-f29e-4f1f-87a3-09f9cf0fb70c"

# WAF Configuration
web_acl_id = "arn:aws:wafv2:us-east-1:043748923082:global/webacl/CreatedByCloudFront-d198945c/a5351067-a055-4830-8622-8a6d645910a3"
